<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="left_Text" format="string" />
    <attr name="right_Text" format="string" />
    <attr name="right_HintText" format="string" />
    <attr name="right_drawableRight" format="reference" />
    <attr name="right_weight" format="float" />

    <attr name="left_Image" format="reference" />
    <attr name="right_Image" format="reference" />

    <declare-styleable name="AddView">
        <attr name="left_Text" />
        <attr name="right_Text" />
        <attr name="right_HintText" />
        <attr name="right_drawableRight" />
        <attr name="right_weight" />
        <attr name="type">
            <enum name="input" value="100" />
            <enum name="click" value="101" />
            <enum name="no" value="102" />
            <enum name="spinner" value="103" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MyItemView">
        <attr name="left_Text" />
        <attr name="right_Text" />
        <attr name="right_HintText" />
        <attr name="left_Image" />
        <attr name="right_Image" />
        <attr name="android:inputType" />
        <attr name="top_line" format="boolean" />
        <attr name="bottom_line" format="boolean" />
        <attr name="right_text_type">
            <enum name="text" value="100" />
            <enum name="edit" value="101" />
        </attr>
    </declare-styleable>

</resources>